{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020", "esnext"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false
  },
  "exclude": ["frontend"]
}
